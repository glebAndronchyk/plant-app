{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["./"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@styled": ["./src/components/styled/index"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"],
      "@icons/*": ["./src/assets/icons/*"],
      "@app_types/*": ["./src/types/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@navigators": ["./src/navigators/index"],
      "@store/*": ["./src/store/*"],
      "@animations/*": ["./src/animations/*"]
    }
  }
}
